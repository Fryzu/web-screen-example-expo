{
  "expo": {
    "name": "webscreenexampleexpo",
    "slug": "webscreenexampleexpo",
    "scheme": "webscreenexampleexpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webscreenexampleexpo.webscreenexampleexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.webscreenexampleexpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
